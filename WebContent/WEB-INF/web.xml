<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0" id="WebApp_ID"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<display-name>Cindy's Resume Appointment Scheduler</display-name>
	<servlet>
		<servlet-name>cindy-resume</servlet-name>
		<servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>

	<servlet-mapping>
		<servlet-name>cindy-resume</servlet-name>
		<url-pattern>/appointment_scheduler.jsp</url-pattern>
		<url-pattern>/images/cindys_resume.jfif</url-pattern>
		<url-pattern>/static/css/app.css</url-pattern>
		<url-pattern>/index.jsp</url-pattern>
		<url-pattern>/appointment_scheduler.html</url-pattern>
		<url-pattern>/appointment_confirmation.jsp</url-pattern>
		<url-pattern>/appointment_confirmation.html</url-pattern>
		<url-pattern>*.html</url-pattern>
	</servlet-mapping>

</web-app>